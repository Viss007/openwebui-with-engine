# OpenWebUI Engine Configuration
# Copy this file to .env and fill in your values

# Server Configuration
PORT=8080
DATA_DIR=/data

# OpenAI Configuration (Required for chat functionality)
OPENAI_API_KEY=sk-your-api-key-here
OPENAI_MODEL=gpt-4o-mini
OPENAI_BASE_URL=https://api.openai.com/v1

# Engine MCP Configuration (Optional - for Railway-hosted MCP integration)
ENGINE_MCP_HTTP_URL=https://your-mcp-service.railway.app/mcp
ENGINE_MCP_SSE_URL=https://your-mcp-service.railway.app/sse
ENGINE_MCP_TOKEN=your-bearer-token-here

# Logging
ENGINE_LOG_LEVEL=INFO

# Python Path (for Open WebUI imports in Docker)
PYTHONPATH=/app/backend:/app
